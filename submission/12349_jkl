#include<iostream>
using namespace std;

void solve() 
{
    int n, m, d = 0;
    cin >> n >> m;
    int a[n];
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
        d += a[i];
    }
        

    if(d - m > 0) cout << d - m << endl;
    else cout << 0 << endl;
}

int main()
{
	int t;
	cin >> t;
	while (t--)
		solve();
}